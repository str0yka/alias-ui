{
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "useTabs": false,
  "printWidth": 100,
  "trailingComma": "none",
  "semi": true,
  "singleAttributePerLine": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": ["<THIRD_PARTY_MODULES>", "^@/(.*)$", "^../", "^./"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
